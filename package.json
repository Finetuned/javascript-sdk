{
  "name": "xtralife-js-client",
  "version": "3.1.0",
  "description": "Simple javascript client for Xtralife gaming backend",
  "main": "lib/bundle.js",
  "scripts": {
    "test": "mocha test/*.coffee",
    "compile": "browserify -t coffeeify src/index.coffee > ./lib/bundle.js && uglifyjs -c -- lib/bundle.js > lib/bundle.min.js",
    "prepublish": "npm run compile",
    "start": "http-server sample"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clanofthecloud/javascript-client.git"
  },
  "author": "chris@xtralife.cloud",
  "dependencies": {
    "superagent": "=1.2.0",
    "agentkeepalive": "*"
  },
  "engines": {
    "node": ">= 0.10.26"
  },
  "devDependencies": {
    "coffee-script": "*",
    "coffee-script-mapped": "*",
    "randomstring": "*",
    "should": "*",
    "mocha": "*",
    "async": "*",
    "q": "*",
    "browserify": "*",
    "coffeeify": "*",
    "uglify-js": "*",
    "http-server": "*"
  },
  "readmeFilename": "Readme.md"
}
