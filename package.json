    "superagent": "^2.3.0"
  },
  "engines": {
    "node": ">= 0.10.26"
  },
  "devDependencies": {
    "async": "^3.2.0",
    "browserify": "^16.5.1",
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "q": "*",
    "randomstring": "*",
    "should": "*",
    "uglify-js": "^3.9.4"
  },
  "readmeFilename": "Readme.md",
